    <main class="ps-0 ps-md-5 flex-grow-1 " style="z-index: 9;" >
      <%= yield %>
      
    </main>
    
  </div>
 
     <div class="col-12" style="z-index: 10;">
        <%= render "shared/footer" %>
      </div>

    
  </body>
</html>
